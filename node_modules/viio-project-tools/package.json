{
  "name": "viio-project-tools",
  "version": "1.0.36",
  "description": "Tools for VIIO developments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc --skipLibCheck",
    "publish-lib": "npm run build && npm publish",
    "publish-beta": "npm run build && npm publish --tag beta",
    "publish-dryrun": "npm run build && npm publish --dry-run",
    "link": "npm run build && npm link",
    "test": "npm run build && jest --runInBand --detectOpenHandles",
    "test-watch": "npm run build && jest --watch --runInBand --detectOpenHandles",
    "test-silent": "npm run build && jest --runInBand --detectOpenHandles --silent",
    "test-coverage": "npm run build && jest --runInBand --detectOpenHandles --silent --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ITRMachines/viio-project-tools.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ITRMachines/viio-project-tools/issues"
  },
  "homepage": "https://github.com/ITRMachines/viio-project-tools#readme",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.7",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "itrm-tools": "^1.0.46",
    "morgan": "^1.10.0"
  },
  "optionalDependencies": {
    "axios-mock-adapter": "^1.22.0"
  }
}
